{
	"name": "path-fixxer",
	"version": "0.3.2",
	"description": "Npm package path fixer for chrome extensions",
	"license": "MIT",
	"author": {
		"name": "Anil Kumar"
	},
	"main": "main.js",
	"type": "module",
	"exports": "./main.js",
	"engines": {
		"node": ">=16.14"
	},
	"repository": "anilkumarum/path-fixxer",
